<svg width="<%= icon.width%>"
     height="<%= icon.height%>"
        <% if (icon['viewBox'] !== undefined) { %> viewBox="<%=icon.fill %>" <% } %>
     fill="<%= icon.fill%>"
     xmlns="<%= icon.xmlns%>">
    <% icon.paths.forEach(path => { %>
        <path d="<%=path.d %>"
              <% if (path['fill'] !== undefined) { %> fill=<%=path.fill %> <% } %>
              <% if (path['stroke'] !== undefined) { %> stroke=<%=path.stroke %> <% } %>
              <% if (path['strokeWidth'] !== undefined) { %> stroke-width=<%=path.strokeWidth %> <% } %>
              <% if (path['strokeMiterlimit'] !== undefined) { %> stroke-miterlimit=<%=path.strokeMiterlimit %> <% } %>
              <% if (path['strokeLinecap'] !== undefined) { %> stroke-linecap=<%=path.strokeLinecap %> <% } %>
              <% if (path['strokeLinejoin'] !== undefined) { %> stroke-linejoin=<%=path.strokeLinejoin %> <% } %>
        />
    <% }); %>
</svg>